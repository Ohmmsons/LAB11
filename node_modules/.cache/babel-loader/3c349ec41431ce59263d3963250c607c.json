,CAAA,YAAA,GAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA,CAJA;;AAAA;AAAA;AAAA;;AAAA;AAMA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,UAAA,GAAA,KAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA7BA;AAgCA,IAAA,MAhCA,oBAgCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AAlCA,GAfA;AAoDA,EAAA,QAAA,EAAA;AACA,IAAA,eAAA,EAAA,2BAAA;AACA,aAAA,KAAA,IAAA,CAAA,QAAA,KAAA,EAAA;AACA,KAHA;AAKA,IAAA,YAAA,EAAA,wBAAA;AACA,aAAA,KAAA,IAAA,CAAA,KAAA,KAAA,EAAA,IAAA,KAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,GAAA,MAAA,CAAA,CAAA;AACA;AAPA,GApDA;AA6DA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA;AACA,MAAA,QADA,oBACA,EADA,EACA;AACA,QAAA,EAAA,CAAA,KAAA;AACA;AAHA;AADA,GA7DA;AAoEA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,UAAA,GAAA,KAAA;AACA,SAAA,KAAA,GAAA,KAAA;AACA;AAvEA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <Menu/>\r\n    <br><br>\r\n    <h1 style=\"text-align: center\">Login</h1>\r\n    <form class=\"form-group\" @submit.prevent=\"handleSubmit\" @keydown.enter.exact.prevent=\"handleSubmit\">\r\n      <label>Email</label>\r\n      <input\r\n          class = \"form-control\"\r\n          type=\"email\"\r\n          placeholder=\"Please enter a valid email\"\r\n          :class=\"{ 'has-error': submitting && invalidEmail }\"\r\n          v-model=\"user.email\"\r\n          @focus=\"clearStatus\"\r\n          ref=\"first\"\r\n          v-autofocus\r\n          @keypress=\"clearStatus\"\r\n      />\r\n\r\n      <label>Password</label>\r\n      <input\r\n          class = \"form-control\"\r\n          type=\"password\"\r\n          :class=\"{ 'has-error': submitting && invalidPassword }\"\r\n          v-model=\"user.password\"\r\n          @focus=\"clearStatus\"\r\n      />\r\n      <p v-if=\"error && submitting\" class=\"error-message\">\r\n        Please fill out all required fields\r\n      </p>\r\n      <br>\r\n      <h3 style=\"text-align:center;\">\r\n        <button class=\"btn\" @click=\"cancel()\" >Cancel</button>\r\n        <button class=\"btn\" type=\"submit\">Login</button>\r\n      </h3>\r\n      <br><br>\r\n      <h3 style=\"text-align: center\"> Not yet registered? Register\r\n        <router-link to=\"/register\">here</router-link>\r\n      </h3>\r\n\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport Menu from \"@/components/Header\";\r\nexport default {\r\n  components: {Menu},\r\n  data() {\r\n    return {\r\n      user: {\r\n        id: '',\r\n        name: '',\r\n        email: '',\r\n        session_id: '',\r\n      },\r\n      submitting: false,\r\n      error: false,\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    handleSubmit: function () {\r\n      this.submitting = true\r\n      this.clearStatus()\r\n\r\n      if (this.invalidEmail || this.invalidPassword) {\r\n        this.error = true\r\n        return\r\n      }\r\n\r\n      this.loginUser(this.user)\r\n\r\n\r\n    },\r\n    clearStatus: function () {\r\n      this.error = false\r\n    },\r\n\r\n    async loginUser(user) {\r\n      if (await this.$store.dispatch('user/loginUser', user)) {\r\n        //login valid\r\n        this.userLoggedIn = true;\r\n        await this.$router.push('/message/5')\r\n      } else {\r\n        //login failed\r\n        this.error = true\r\n        this.submitting = false\r\n      }\r\n\r\n    },\r\n\r\n\r\n    cancel() {\r\n      this.$router.push('/')\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    invalidPassword: function () {\r\n      return this.user.password === '';\r\n    },\r\n\r\n    invalidEmail: function () {\r\n      return this.user.email === '' || this.user.email.search('@') === -1;\r\n    },\r\n  },\r\n  directives: {\r\n    autofocus: {\r\n      inserted(el) {\r\n        el.focus()\r\n      }\r\n    }\r\n  },\r\n  created: function () {\r\n    this.submitting = false\r\n    this.error = false\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views"}]}