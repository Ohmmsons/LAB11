Decrement) {\r\n            state.products.forEach( function (product, index) {\r\n                if (product.id == idToDecrement && product.quantity >= 1)\r\n                    product.quantity--\r\n                // remove item if quantity is 0\r\n                if (product.quantity == 0)\r\n                    state.products.splice(index,1)\r\n            })\r\n        },\r\n    },\r\n    actions: {\r\n    },\r\n    modules: {\r\n    }\r\n}\r\nexport default\r\nbasket"]}]}